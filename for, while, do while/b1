#include <stdio.h>

int main() {
    int numbers[10];
    int sum = 0;

    printf("Nhap 10 so nguyen:\n");

    for (int i = 0; i < 10; i++) {
        printf("So thu %d: ", i + 1);
        scanf("%d", &numbers[i]);
        sum += numbers[i];
    }

    printf("Tong cua 10 so nguyen da nhap la: %d\n", sum);

    return 0;
}
